<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:padding="10dp">
    <ImageButton
        android:src="@android:drawable/ic_menu_gallery"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton1" />
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout1">
        <TextView
            android:text="Age"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_alignBaseline="@+id/SignUpPage4Age" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Age"
            android:layout_toRightOf="@+id/textView1" />
        <TextView
            android:text="University"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:layout_below="@+id/SignUpPage4Age"
            android:layout_alignBaseline="@+id/SignUpPage4University" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4University"
            android:layout_toRightOf="@+id/textView2"
            android:layout_below="@+id/SignUpPage4Age" />
        <TextView
            android:text="High School"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:layout_below="@+id/SignUpPage4University"
            android:layout_alignBaseline="@+id/SignUpPage4HighSchool" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4HighSchool"
            android:layout_below="@+id/SignUpPage4University"
            android:layout_toRightOf="@+id/textView3" />
        <TextView
            android:text="Middle School"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:layout_below="@+id/SignUpPage4HighSchool"
            android:layout_marginTop="5dp"
            android:layout_alignBaseline="@+id/SignUpPage4MiddleSchool" />
        <TextView
            android:text="//Personal Information//"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:layout_centerInParent="true"
            android:gravity="bottom"
            android:layout_above="@+id/button1"
            android:layout_marginBottom="20dp" />
        <Button
            android:text="Next"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:id="@+id/button1"
            android:layout_alignParentBottom="true" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4MiddleSchool"
            android:layout_toRightOf="@+id/textView4"
            android:layout_below="@+id/SignUpPage4HighSchool" />
        <TextView
            android:text="Ethnicity"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:layout_below="@+id/SignUpPage4MiddleSchool" />
        <CheckBox
            android:text="Asian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Asian"
            android:layout_below="@+id/textView6"
            android:textSize="12dp" />
        <CheckBox
            android:text="Black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Black"
            android:layout_below="@+id/SignUpPage4Asian"
            android:textSize="12dp" />
        <CheckBox
            android:text="Caucasian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Caucasian"
            android:layout_below="@+id/SignUpPage4Black"
            android:textSize="12dp" />
        <CheckBox
            android:text="Indian"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Indian"
            android:layout_below="@+id/textView6"
            android:textSize="12dp"
            android:layout_toRightOf="@+id/SignUpPage4Asian"
            android:layout_marginLeft="40dp"
            android:layout_width="wrap_content" />
        <CheckBox
            android:text="Latin/Hispanic"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Latin"
            android:layout_below="@+id/SignUpPage4Asian"
            android:textSize="12dp"
            android:layout_toRightOf="@+id/SignUpPage4Black"
            android:layout_marginLeft="40dp"
            android:layout_width="wrap_content" />
        <CheckBox
            android:text="Middle Eastern"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Middle"
            android:layout_below="@+id/SignUpPage4Black"
            android:textSize="12dp"
            android:layout_toRightOf="@+id/SignUpPage4Black"
            android:layout_marginLeft="40dp"
            android:layout_width="wrap_content" />
        <CheckBox
            android:text="Other"
            android:layout_height="wrap_content"
            android:id="@+id/SignUpPage4Other"
            android:layout_below="@+id/textView6"
            android:textSize="12dp"
            android:layout_toRightOf="@+id/SignUpPage4Indian"
            android:layout_marginLeft="40dp"
            android:layout_width="wrap_content" />
    </RelativeLayout>
</LinearLayout>